{
  "name": "parse-release-tag",
  "version": "0.0.1",
  "description": "Parse a release tag into its components",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build ./src/main.ts --source-map --license licenses.txt",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts && npx tsc --noEmit",
    "test": "mocha -r ts-node/register '__tests__/**/*.ts'",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realm/ci-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "realm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.5",
    "@typescript-eslint/parser": "^5.33.1",
    "@vercel/ncc": "^0.34.0",
    "chai": "^4.3.6",
    "eslint": "^8.22.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^5.0.0",
    "js-yaml": "^4.1.0",
    "mocha": "^10.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
